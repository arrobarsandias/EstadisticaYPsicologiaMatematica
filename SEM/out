	Attitude -> Intent [label=""]
	SubNorm -> Intent [label=""]
	PBC -> Intent [label=""]
	e2 -> Behavior [label=""]
	PBC -> Behavior [label=""]
	Intent -> Behavior [label=""]
	PBC -> SubNorm [label="" dir=both style=dashed]
	PBC -> Attitude [label="" dir=both style=dashed]
	SubNorm -> Attitude [label="" dir=both style=dashed]
	e2 -> e1 [label="" dir=both style=dashed]
}
